#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int main()
{

while(1)
{

char input[MAX_INPUT_SIZE]; // Buffer storage for user input

// Start ui loop
   printf("> "); // Display prompt for user input
   
//Get input from the user
if (fgets(input, MAX_INPUT_SIZE, stdin) == NULL)
{
// If fgets fails
   printf("Error reading input.\n");

}   

}

}
